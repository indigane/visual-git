{
  "name": "Visual Git Container",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "workspaceFolder": "/home/user/app",
  "remoteUser": "user",
  "customizations": {
    "vscode": {
      "extensions": []
    }
  },
  "mounts": [
    "source=${localEnv:HOME}/.gitconfig,target=/home/user/.gitconfig,type=bind,consistency=cached",
    "source=${localWorkspaceFolder},target=/home/user/app,type=bind,consistency=cached"
  ]
}
